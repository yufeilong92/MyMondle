<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <declare-styleable name="RoundLoadingBar">
        <attr name="bigcircle_radius" format="dimension" />
        <attr name="smallcircle_radius" format="dimension" />
        <attr name="frame_angle" format="dimension" />
        <attr name="frame_alpha" format="float" />
        <attr name="smallcircle_color" format="color" />
        <attr name="smallcircle_alpha" format="float" />
        <attr name="frame_color" format="color" />
    </declare-styleable>
<style name="AppthemeNoAction" parent="Theme.AppCompat.NoActionBar">
   <item name="colorPrimary">@color/colorPrimary</item>
    <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    <item name="colorAccent">@color/colorAccent</item>
</style>
</resources>
